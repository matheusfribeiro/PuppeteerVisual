  "bugs": {
    "url": "https://github.com/matheusfribeiro/betCrawler/issues"
  },
  "homepage": "https://github.com/matheusfribeiro/betCrawler#readme",
  "dependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "puppeteer": "^20.2.1"
  }
}
